# Firebase Configuration
# Copy this file to .env and fill in your Firebase project details
# Get these from your Firebase Console: https://console.firebase.google.com/

VITE_FIREBASE_CONFIG={"apiKey":"YOUR_API_KEY","authDomain":"YOUR_PROJECT_ID.firebaseapp.com","projectId":"YOUR_PROJECT_ID","storageBucket":"YOUR_PROJECT_ID.appspot.com","messagingSenderId":"YOUR_MESSAGING_SENDER_ID","appId":"YOUR_APP_ID"}

# App ID (can be any unique identifier for your app)
VITE_APP_ID=lazy-training-app

# Gemini API Key (for AI plan generation)
# Get from: https://makersuite.google.com/app/apikey
VITE_GEMINI_API_KEY=YOUR_GEMINI_API_KEY
