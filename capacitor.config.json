{
  "appId": "com.lazytraining.app",
  "appName": "Lazy Training",
  "webDir": "dist",
  "bundledWebRuntime": false,
  "backgroundColor": "#1f2937",
  "android": {
    "allowMixedContent": true
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#1f2937",
      "showSpinner": true,
      "androidSpinnerStyle": "large",
      "spinnerColor": "#6366f1"
    }
  }
}
